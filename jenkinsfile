pipeline {
    agent none

    stages{   
        stage('Clone Backend Repository for DEV environment') {
            agent { label 'debian' }
            steps {
                git branch: 'main', url: 'https://github.com/benyer10repo/backendm8.git'
                echo "clonaci√≥n finalizada"
            }
        }
        stage('Build backend for DEV with docker-compose') {
            agent { label 'debian' }
            steps {
                sh 'docker-compose up -d'
            }
        }
        
    }
}